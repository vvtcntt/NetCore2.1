@{
    ViewData["Title"] = "Index";
}
@section Scripts{
    <script src="~/app/controllers/function/index.js"  asp-append-version="true"></script>

    <script src="~/vendor/jquery-easyui/js/jquery.easyui.min.js"></script>
    <script>
        var functions = new functionController();
        functions.initialize();
    </script>

}
@section Styles{
    <link href="~/vendor/jquery-easyui/css/easyui.css" rel="stylesheet" />
}
<div class="page-title">
    <div class="title_left">
        <h3>Function <small>List Function</small></h3>
    </div>
    <div class="title_right">
        <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
            <button class="btn btn-success" id="btnCreate" type="button">Add</button>

        </div>

    </div>
</div>
<div class="clearfix"></div>
<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">

                <ul class="nav navbar-right panel_toolbox">
                    <li>
                        <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                        <ul class="dropdown-menu" role="menu">
                            <li>
                                <a href="#">Settings 1</a>
                            </li>
                            <li>
                                <a href="#">Settings 2</a>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a class="close-link"><i class="fa fa-close"></i></a>
                    </li>
                </ul>
                <div class="clearfix"></div>
            </div>
            <div class="x_content">

                <ul id="treeProductCategory"></ul>
                <div id="contextMenu" class="easyui-menu" style="width:120px;">
                    <div id="btnEdit" data-options="iconCls:'icon-edit'">Edit</div>
                    <div id="btnCreateM" data-options="iconCls:'icon-add'">Add</div>
                    <div id="btnDelete" data-options="iconCls:'icon-remove'">Delete</div>
                </div>
            </div>
        </div>
    </div>
</div>

